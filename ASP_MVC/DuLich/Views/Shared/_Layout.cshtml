<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - DuLich</title>
    <script type="importmap"></script>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/DuLich.styles.css" asp-append-version="true" />
    @RenderSection("Head", required: false)
</head>

<body>
    @await Html.PartialAsync("_CustomerHeader")
    @await Html.PartialAsync("_CustomerBanner")

    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    @await Html.PartialAsync("_CustomerFooter")
    @await RenderSectionAsync("Scripts", required: false)

</body>

</html>